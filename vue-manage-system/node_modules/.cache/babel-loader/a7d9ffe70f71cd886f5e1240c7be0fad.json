功！');\r\n\r\n        },\r\n        xinZengF(){\r\n        this.$router.push('/upload');\r\n        }\r\n    },\r\n    created () {\r\n    this.axios.get('/')\r\n      .then(function (response) {\r\n        console.log(response)\r\n      })\r\n      .catch(function (response) {\r\n        console.log(response)\r\n      })\r\n    }\r\n};\r\n</script>"],"sourceRoot":"src/components/page"}]}