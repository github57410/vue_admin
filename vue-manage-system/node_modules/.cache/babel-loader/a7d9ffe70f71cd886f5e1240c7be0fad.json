 <el-date-picker\r\n                                type=\"date\"\r\n                                placeholder=\"选择日期\"\r\n                                v-model=\"form.date1\"\r\n                                value-format=\"yyyy-MM-dd\"\r\n                                style=\"width: 100%;\"\r\n                            ></el-date-picker>\r\n                        </el-col>\r\n                        <el-col class=\"line\" :span=\"2\">-</el-col>\r\n                        <el-col :span=\"11\">\r\n                            <el-time-picker\r\n                                placeholder=\"选择时间\"\r\n                                v-model=\"form.date2\"\r\n                                style=\"width: 100%;\"\r\n                            ></el-time-picker>\r\n                        </el-col>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"城市级联\">\r\n                        <el-cascader :options=\"options\" v-model=\"form.options\"></el-cascader>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"文本框\">\r\n                        <el-input type=\"textarea\" rows=\"5\" v-model=\"form.desc\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item>\r\n                        <el-button type=\"primary\" @click=\"onSubmit\">表单提交</el-button>\r\n                        <el-button>取消</el-button>\r\n                    </el-form-item>\r\n                </el-form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'baseform',\r\n    data() {\r\n        return {\r\n            options: [\r\n                {\r\n                    value: 'guangdong',\r\n                    label: '广东省',\r\n                    children: [\r\n                        {\r\n                            value: 'guangzhou',\r\n                            label: '广州市',\r\n                            children: [\r\n      